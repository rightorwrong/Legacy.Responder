{
	"name": "EvolveCMS/Legacy.Responder",
	"description": "Legacy WordPress Views Refactored from the source.",
	"minimum-stability": "dev",
	"license": "GPLv2",
	"require": {
		"EvolveCMS-Project/Support": "dev-master"
	},
	"require-dev": {
		"codeclimate/php-test-reporter": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"EvolveCMS\\Responder\\WP\\": "src/"
		},
		"files": [
			"src/functions.php",
			"src/Domain/functions.php",
			"src/Render/functions.php",
			"src/Resolver/functions.php"
		]
	},
	"respositories": [
		{
			"type": "vcs",
			"url": "https://github.com/EvolveCMS-Project/Support"
		}
	]
}
